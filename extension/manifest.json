{
    "name": "Github Nofitication Extension",
    "description": "Notifies user when commit or pull request is made.",
    "version": "1.0",
    "manifest_version": 2,
    "icons": {
        "16": "./gitlogo16.png",
        "32": "./gitlogo32.png",
        "48": "./gitlogo48.png",
        "128": "./gitlogo128.png"
    },
    "background": {
        "scripts": ["./background.js"]
    },
    "options_page": "./options.html",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "*://*/*",
        "storage"
    ]
}